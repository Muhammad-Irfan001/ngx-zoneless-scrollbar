{
  "name": "ngx-zoneless-scrollbar-workspace",
  "version": "1.0.0",
  "description": "Workspace for ngx-zoneless-scrollbar library",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "ng build ngx-zoneless-scrollbar --configuration=production",
    "build:watch": "ng build ngx-zoneless-scrollbar --watch",
    "test": "ng test ngx-zoneless-scrollbar",
    "prepublish:lib": "npm run build",
    "publish:lib": "cd dist/ngx-zoneless-scrollbar && npm publish --access public"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.0",
    "@angular/cli": "^19.0.0",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.0.0",
    "@angular/compiler-cli": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@types/node": "^20.0.0",
    "ng-packagr": "^19.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "typescript": "~5.6.0",
    "zone.js": "~0.15.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Legalfina/ngx-zoneless-scrollbar"
  },
  "bugs": {
    "url": "https://github.com/Legalfina/ngx-zoneless-scrollbar/issues"
  },
  "homepage": "https://github.com/Legalfina/ngx-zoneless-scrollbar#readme"
